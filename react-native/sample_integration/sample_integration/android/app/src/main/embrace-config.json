
{
  "app_id": "YOUR_APP_ID",
  "api_token": "YOUR_API_TOKEN",
  "sdk_config": {
    "networking": {
      "trace_id_header": "test_trace_id_header",
      "default_capture_limit": 10
    },
    "session": {
